{
  "name": "evaengine",
  "version": "0.0.27",
  "description": "EvaEngine For NodeJS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/EvaEngine/EvaNode.git"
  },
  "scripts": {
    "prestart": "npm run build",
    "prepublish": "rm -rf ./lib && npm run build",
    "ava": "LOG_LEVEL=error NODE_ENV=test ./node_modules/.bin/cross-env node --harmony --harmony-proxies ./node_modules/.bin/ava --verbose --serial",
    "test": "nyc -a --reporter=lcov --reporter=text --reporter=html npm run ava",
    "lint": "./node_modules/.bin/eslint src/",
    "build": "babel -d lib/ src/"
  },
  "engines": {
    "node": ">=4.4.5"
  },
  "keywords": [
    "Eva",
    "Engine"
  ],
  "main": "index.js",
  "author": "AlloVince",
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/_*/**/*.js",
      "!test/swagger/_*/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": false,
    "tap": false,
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "include": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "acorn": "^3.2.0",
    "acorn-es7-plugin": "^1.0.17",
    "app-root-path": "^1.2.0",
    "bluebird": "^3.4.0",
    "connect-redis": "^3.1.0",
    "constitute": "^1.6.2",
    "doctrine": "^1.2.2",
    "eslint-plugin-import": "^1.11.1",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "glob": "^7.0.4",
    "js-yaml": "^3.6.1",
    "jwt-simple": "^0.5.0",
    "later": "^1.2.0",
    "lodash": "^4.13.0",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "node-mocks-http": "^1.5.2",
    "pug": "^2.0.0-alpha8",
    "redis": "^2.6.2",
    "request": "^2.73.0",
    "request-debug": "^0.2.0",
    "request-promise": "^4.0.2",
    "sequelize": "^3.23.6",
    "shortid": "^2.2.6",
    "stow": "^1.0.1",
    "swagger-ui": "^2.1.8-M1",
    "winston": "^2.2.0",
    "yargs": "^4.8.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-3": "^6.11.0",
    "cross-env": "^2.0.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.1",
    "gulp-cli": "^1.2.1",
    "gulp-sourcemaps": "^1.6.0",
    "harmony-reflect": "^1.4.6",
    "nyc": "^7.0.0"
  }
}
