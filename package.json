{
  "name": "evaengine",
  "version": "0.0.19",
  "description": "EvaEngine For NodeJS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/EvaEngine/EvaNode.git"
  },
  "scripts": {
    "prestart": "npm run build",
    "prepublish": "rm -rf ./lib && npm run build",
    "test-raw": "NODE_ENV=test node --harmony ./node_modules/.bin/mocha --harmony --compilers js:babel-core/register --require babel-polyfill --recursive",
