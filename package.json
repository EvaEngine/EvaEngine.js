    "lint": "./node_modules/.bin/eslint src/",
    "build": "babel -d lib/ src/"
  },
  "engines": {
    "node": ">=4.4.2"
  },
  "keywords": [
    "Eva",
    "Engine"
  ],
  "main": "index.js",
  "author": "AlloVince",
  "dependencies": {
