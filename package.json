    "connect-redis": "^3.1.0",
    "constitute": "^1.6.2",
    "doctrine": "^1.2.2",
    "express": "^4.14.0",
    "express-session": "^1.13.0",
    "glob": "^7.0.4",
    "js-yaml": "^3.6.1",
    "jwt-simple": "^0.5.0",
    "later": "^1.2.0",
    "lodash": "^4.13.0",
    "moment": "^2.13.0",
