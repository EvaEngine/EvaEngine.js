{
  "name": "evaengine",
  "version": "0.2.0",
  "description": "EvaEngine for Node.js, a micro service development engine",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/EvaEngine/EvaEngine.js.git"
  },
  "scripts": {
    "prestart": "npm run build",
    "prepublish": "rm -rf ./lib && npm run build",
    "ava": "LOG_LEVEL=error NODE_ENV=test node ./node_modules/.bin/ava --verbose --serial",
    "ava_v4": "LOG_LEVEL=error NODE_ENV=test ./node_modules/.bin/cross-env node --harmony --harmony-proxies ./node_modules/.bin/ava --verbose --serial",
    "test": "nyc -a --reporter=lcov --reporter=text --reporter=html npm run ava",
    "lint": "./node_modules/.bin/eslint src/",
    "build": "babel -d lib/ src/ --source-maps"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "es7",
    "es2015",
    "engine",
    "micro service",
    "orm"
  ],
  "main": "index.js",
  "bin": {
    "engine": "./bin/engine"
  },
  "author": "AlloVince",
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/bootstrap.js",
      "!test/_*/**/*.js",
      "!test/swagger/_*/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": false,
    "tap": false,
    "require": [
      "babel-core/register",
      "babel-polyfill",
      "./test/bootstrap"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/config/index.js"
    ]
  },
  "dependencies": {
    "acorn": "^4.0.0",
    "app-root-path": "^2.0.1",
    "bluebird": "^3.4.6",
    "connect-redis": "^3.1.0",
    "constitute": "^1.6.2",
    "continuation-local-storage": "^3.2.0",
    "doctrine": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "glob": "^7.1.0",
    "ioredis": "^2.4.3",
    "joi": "^10.0.6",
    "js-yaml": "^3.7.0",
    "jwt-simple": "^0.5.1",
    "later": "^1.2.0",
    "lodash": "^4.17.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.17.1",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "node-mocks-http": "^1.5.5",
    "on-headers": "^1.0.1",
    "pug": "^2.0.0-beta6",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3",
    "sequelize": "^3.27.0",
    "swagger-ui": "^2.2.6",
    "winston": "^2.2.0",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-3": "^6.17.0",
    "cross-env": "^3.1.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.8.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.2.2",
    "gulp-sourcemaps": "^2.2.0",
    "harmony-reflect": "^1.5.0",
    "is-type-of": "^1.0.0",
    "nock": "^9.0.0",
    "nyc": "^10.0.0"
  }
}
