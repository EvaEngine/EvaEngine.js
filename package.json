{
  "name": "evaengine",
  "version": "0.1.11",
  "description": "EvaEngine For NodeJS, a micro service development engine",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/EvaEngine/EvaEngine.js.git"
  },
  "scripts": {
    "prestart": "npm run build",
    "prepublish": "rm -rf ./lib && npm run build",
    "ava": "LOG_LEVEL=error NODE_ENV=test ./node_modules/.bin/cross-env node --harmony --harmony-proxies ./node_modules/.bin/ava --verbose --serial",
    "test": "nyc -a --reporter=lcov --reporter=text --reporter=html npm run ava",
    "lint": "./node_modules/.bin/eslint src/",
    "build": "babel -d lib/ src/"
  },
  "engines": {
    "node": ">=4.4.5"
  },
  "keywords": [
    "es7",
    "es2015",
    "engine",
    "micro service",
    "orm"
  ],
  "main": "index.js",
  "bin": {
    "engine": "./bin/engine"
  },
  "author": "AlloVince",
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/bootstrap.js",
      "!test/_*/**/*.js",
      "!test/swagger/_*/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": false,
    "tap": false,
    "require": [
      "babel-core/register",
      "babel-polyfill",
      "./test/bootstrap"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/config/index.js"
    ]
  },
  "dependencies": {
    "acorn": "^3.3.0",
    "acorn-es7-plugin": "^1.0.17",
    "app-root-path": "^1.3.0",
    "bluebird": "^3.4.1",
    "connect-redis": "^3.1.0",
    "constitute": "^1.6.2",
    "continuation-local-storage": "^3.1.7",
    "doctrine": "^1.2.2",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "glob": "^7.0.5",
    "ioredis": "^2.3.0",
    "js-yaml": "^3.6.1",
    "jwt-simple": "^0.5.0",
    "later": "^1.2.0",
    "lodash": "^4.15.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "node-mocks-http": "^1.5.3",
    "on-headers": "^1.0.1",
    "pug": "^2.0.0-alpha8",
    "request": "^2.74.0",
    "request-promise": "^4.1.0",
    "sequelize": "^3.24.0",
    "swagger-ui": "^2.2.0",
    "winston": "^2.2.0",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-3": "^6.11.0",
    "cross-env": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.1",
    "gulp-cli": "^1.2.1",
    "gulp-sourcemaps": "^1.6.0",
    "harmony-reflect": "^1.4.6",
    "nyc": "^8.1.0"
  }
}
