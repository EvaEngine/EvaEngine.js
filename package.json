{
  "name": "evaengine",
  "version": "0.4.2",
  "description": "EvaEngine for Node.js, a micro service development engine",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/EvaEngine/EvaEngine.js.git"
  },
  "scripts": {
    "prestart": "npm run build",
    "prepublish": "rm -rf ./lib && npm run build",
    "ava": "LOG_LEVEL=error NODE_ENV=test ava --verbose --serial",
    "test": "nyc -a --reporter=lcov --reporter=text --reporter=html npm run ava",
    "lint": "eslint src/* --ext .js",
    "build": "gulp build"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "es7",
    "es2015",
    "engine",
    "micro service",
    "orm"
  ],
  "main": "index.js",
  "bin": {
    "engine": "./bin/engine",
    "engine-babel": "./bin/engine-babel"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "AlloVince",
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/bootstrap.js",
      "!test/_*/**/*.js",
      "!test/swagger/_*/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": false,
    "tap": false,
    "require": [
      "babel-core/register",
      "babel-polyfill",
      "./test/bootstrap"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/config/index.js"
    ]
  },
  "dependencies": {
    "acorn": "^5.0.3",
    "app-root-path": "^2.0.1",
    "cloud-config-client": "^1.0.0",
    "connect-redis": "^3.3.0",
    "constitute": "^1.6.2",
    "continuation-local-storage": "^3.2.0",
    "doctrine": "^2.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "glob": "^7.1.2",
    "ioredis": "^3.1.1",
    "joi": "^10.6.0",
    "js-yaml": "^3.8.4",
    "jwt-simple": "^0.5.1",
    "later": "^1.2.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "morgan": "^1.8.2",
    "mysql": "^2.13.0",
    "node-mocks-http": "^1.6.3",
    "on-headers": "^1.0.1",
    "pug": "^2.0.0-beta6",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "sequelize": "^3.30.4",
    "swagger-ui-dist": "^3.0.16",
    "winston": "^2.3.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.3.0",
    "gulp-sourcemaps": "^2.6.0",
    "is-type-of": "^1.0.0",
    "nock": "^9.0.13",
    "nyc": "^11.0.2",
    "pre-commit": "^1.2.2"
  }
}
