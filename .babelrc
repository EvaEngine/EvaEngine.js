{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6.11.0"
        },
        "useBuiltIns": true,
        "debug": false
      }
    ]
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "babel-plugin-transform-builtin-extend",
      {
        "globals": [
          "Error"
        ]
      }
    ]
  ]
}